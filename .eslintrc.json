{
  "env": {
    "commonjs": true,
    "es2021": true,
    "node": true
  },
  "extends": "airbnb-base",
  "parserOptions": {
    "ecmaVersion": "latest"
  },
  "rules": {
    "no-console": "off",
    "no-constant-condition": "off",
    "no-useless-escape": "off",
    "prefer-regex-literals": "off",
    "global-require": "off",
    "prefer-template": "off",
    "object-curly-newline": "off"
  }
}
